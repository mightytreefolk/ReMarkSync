{
  "name": "obsidian-remarkable-sync",
  "version": "1.0.0",
  "description": "Sync ReMarkable tablet notes and PDF annotations to Obsidian as Excalidraw drawings",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production"
  },
  "keywords": [
    "obsidian",
    "remarkable",
    "excalidraw"
  ],
  "author": "Peter Thompson",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.27.1",
    "obsidian": "^1.5.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
